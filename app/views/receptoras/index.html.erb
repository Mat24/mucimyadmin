<%- model_class = Receptora -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:nombre_etidad) %></th>
      <th><%= model_class.human_attribute_name(:tipo_entidad) %></th>
      <th><%= model_class.human_attribute_name(:nombre_receptor) %></th>
      <th><%= model_class.human_attribute_name(:cargo) %></th>
      <th><%= model_class.human_attribute_name(:ciudad) %></th>
      <th><%= model_class.human_attribute_name(:departamento) %></th>
      <th><%= model_class.human_attribute_name(:direccion) %></th>
      <th><%= model_class.human_attribute_name(:telefono) %></th>
      <th><%= model_class.human_attribute_name(:correo_electronico) %></th>
      <th><%= model_class.human_attribute_name(:fecha_recepcion) %></th>
      <th><%= model_class.human_attribute_name(:fecha_valoracion) %></th>
      <th><%= model_class.human_attribute_name(:fecha_elaboracion) %></th>
      <th><%= model_class.human_attribute_name(:fecha_notificacion) %></th>
      <th><%= model_class.human_attribute_name(:numero_dictamen) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @receptoras.each do |receptora| %>
      <tr>
        <td><%= link_to receptora.id, receptora_path(receptora) %></td>
        <td><%= receptora.nombre_etidad %></td>
        <td><%= receptora.tipo_entidad %></td>
        <td><%= receptora.nombre_receptor %></td>
        <td><%= receptora.cargo %></td>
        <td><%= receptora.ciudad %></td>
        <td><%= receptora.departamento %></td>
        <td><%= receptora.direccion %></td>
        <td><%= receptora.telefono %></td>
        <td><%= receptora.correo_electronico %></td>
        <td><%= receptora.fecha_recepcion %></td>
        <td><%= receptora.fecha_valoracion %></td>
        <td><%= receptora.fecha_elaboracion %></td>
        <td><%= receptora.fecha_notificacion %></td>
        <td><%= receptora.numero_dictamen %></td>
        <td><%=l receptora.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_receptora_path(receptora), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      receptora_path(receptora),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-xs btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_receptora_path,
            :class => 'btn btn-primary' %>
